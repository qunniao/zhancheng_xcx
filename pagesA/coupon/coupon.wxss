page{
  background: #EEF2F3;
}

.couponItem{
  width: 90%;
  height: 320rpx;
  margin-left: 5%;
  margin-top: 30rpx;
}

.couponItem image{
  width: 100%;
  height: 100%;
}

.coupon_content{
  display: flex;
}

.price{
  height: 180rpx;
  width: 35%;
  margin-top: -290rpx;
  display: flex;
  flex-direction: column;
  border-right: 1px dashed #D7D5D5;
  text-align: center;
}

.yhPrice{
  padding-top: 20rpx;
}

.amount{
  font-size: 48rpx;
  color: #fff;
}

.yuan{
  font-size: 28rpx;
  color: #fff;
  padding-left: 15rpx;
}

.realPrice{
  font-size: 28rpx;
  color: #fff;
  padding-top: 20rpx;
  letter-spacing: 2rpx;
}

.content{
  width: 65%;
  margin-top: -290rpx;
  display: flex;
  flex-direction: column;
}

.co_content{
  font-size: 24rpx;
  color: #fff;
  line-height: 35rpx;
  padding-left: 10rpx;
}

.content button{
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 10rpx;
  background: #fff;
  border: 0;
  color: #30A2F7;
  font-weight: bold;
  margin-top: 20rpx;
}

.validity{
  font-size: 24rpx;
  color: #797A7B;
  font-weight: bold;
  position: relative;
  top: -66rpx;
  padding-left: 50rpx;
}