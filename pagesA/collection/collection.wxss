.header{
  width: 100%;
  display: flex;
  height: 95rpx;
}

.header text{
  font-size: 34rpx;
  margin-right: 40rpx;
  font-weight: bold;
  letter-spacing: 2rpx;
  margin-left: 30rpx;
}

.collectionItem{
  display: flex;
  width: 100%;
  padding: 30rpx 0;
}

.titleImg{
  width: 280rpx;
  height: 220rpx;
  border-radius: 20rpx;
  margin: 0 20rpx 0 30rpx;
}

.title_period_price{
  display: flex;
  flex-direction: column;
}

.title{
  font-size: 34rpx;
  font-weight: bold;
  letter-spacing: 2rpx;
}

.period{
  font-size: 28rpx;
  color: #797A7B;
  padding-top: 50rpx;
  font-weight: bold;
}

.price_collection{
  display: flex;
}

.price{
  font-size: 24rpx;
  padding-top: 50rpx;
}

.shoucang{
  width: 40rpx;
  height: 40rpx;
  position: relative;
  top: -50rpx;
  left: 350rpx;
}

.flex{
  width: 100%;
  height: 80rpx;
}

.center_content{
  display:flex;
  justify-content: center
}

.tabbar_unselect{
  font-size:24rpx;
  color:#333;
  height:30rpx;
}


.tabbar_select>text{
  font-size:34rpx;
  color:#307bf6;
  border-bottom: 2px solid #307bf6;
  padding-bottom: 20rpx;
  height:30rpx;
}

.ppp{
  margin-top: 25rpx;
}


















