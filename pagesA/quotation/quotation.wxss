page{
  width:100%;
  height:100%;
}
#website{
  width: 100%;
  position: fixed;
  top: 0;
  z-index: 100;
}
.container{
  flex-direction: row;
  height:100%;
  display: flex;
  margin-top: 120rpx;
}
.container-IPX{
  flex-direction: row;
  height:100%;
  display: flex;
  margin-top: 180rpx;
}
.left{
  width:25%;
  height:100%;
  background: #f3f3f3;
  position: fixed;
}
.left scroll-view{
  height:100%;
}
.left view{
  padding:10px;
  text-align: center;
  margin-bottom:10px;
  font-size: 22rpx;
}
.right{
  flex:1;
  width: 75%;
  margin-left: 25%;
  height:100%;
  padding-bottom: 120rpx;
}
.right scroll-view{
  height:100%;
}

.active{
  background: #fff;
  color: #307BF6;
}
.youtitle{
  font-size: 24rpx;
  padding: 10rpx 0;
}

.duoxuan{
  width: 95%;
  margin-left: 5%;
  display: flex;
}

checkbox{
  width: 50%;
  float: left;
  font-size: 22rpx;
  height: 50rpx;
}

checkbox .wx-checkbox-input{
  width: 22rpx;
  height: 22rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
  width: 22rpx;
  height: 22rpx;
  line-height: 22rpx;
  text-align: center;
  font-size:22rpx;
  color:#307BF6;
  border: 1px solid #307BF6;
  border-radius: 4rpx;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

.buzhidao{
  width: 97%;
  margin-left: 3%;
  display: flex;
  flex-direction: column;
  margin-bottom: 20rpx;
}

.youbian{
  border: 1px solid #eee;
  margin-bottom: 20rpx;
}

.active>.diyige{
  padding-left: 20rpx;
  border-left: 2px solid #307BF6;
}

.botoom{
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  position: fixed;
  bottom: 0;
  background: #fff;
  z-index: 100;
  display: flex;
}

.botoom .text{
  font-size: 34rpx;
  color: #4E4C4C;
  font-weight: bold;
  padding-left: 60rpx;
}

.botoom button{
  font-size: 34rpx;
  color: #fff;
  position: absolute;
  right: 60rpx;
  top: 25rpx;
  line-height: 70rpx;
  height: 70rpx;
  padding: 0 40rpx;
  background: #307BF6;
  border-radius: 35rpx;
}
